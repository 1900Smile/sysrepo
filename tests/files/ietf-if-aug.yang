module ietf-if-aug {
    yang-version 1.1;
    namespace "urn:ietf-if-aug";
    prefix ifa;

    import ietf-interfaces {
        prefix if;
    }

    augment "/if:interfaces/if:interface" {
        when "if:type = 'iana-if-type:ethernetCsmacd'";

        container c1 {
            config false;

            leaf oper1 {
                type string;
            }
        }
    }
}
